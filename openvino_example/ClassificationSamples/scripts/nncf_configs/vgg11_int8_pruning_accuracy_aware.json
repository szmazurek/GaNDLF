{
    "model": "vgg11",
    "batch_size" : 256,
    "pretrained": true,

    "root_dir": "/Share/Junwen/UPenn/DFU_Example_vgg11/ClassificationModel/ClassificationModel/",
    "N_FOLD": "0",
    "model_dir":"models/DFU_experiments_vgg11_5fold_without_preprocess/",
    "model_name":"vgg11_best.pth.tar",
    "nncf_data_dir":"scripts/quantization/data/vgg11",
    "nncf_data_name":"patch_samples.npz",

    "epochs": 100,
    "device": "cpu",
    "cpu_only": true,
    "input_info": {
        "sample_size": [
            1,
            3,
            128,
            128
        ]
    },
    "optimizer": {
        "type": "adam",
        "base_lr": 0.001,
        "weight_decay": 5e-5,
        "steps": [
            4764000
        ],
        "optimizer_params":
        {
            "amsgrad": false
        }
    },
    "accuracy_aware_training": {
	    "mode":"adaptive_compression_level",
        "params": {
            "maximal_relative_accuracy_degradation": 1.0,
            "initial_training_phase_epochs": 100,
            "patience_epochs": 30
        }
    },
    "compression": [
       {
            "algorithm": "filter_pruning",
            "pruning_init": 0.5,
            "params": {
                "schedule": "exponential",
                "pruning_target": 0.4,
                 "pruning_steps": 15,
                "filter_importance": "geometric_median"
            }
       },
       {
        "algorithm": "quantization",
        "initializer": {
            "range": {
                "num_init_samples": 850
            }
        } 
       }	
    ]
}
